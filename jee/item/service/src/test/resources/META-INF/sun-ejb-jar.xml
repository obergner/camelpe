<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 EJB 3.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_1-0.dtd">
<sun-ejb-jar>
	<security-role-mapping>
		<role-name>ROLE_ADMIN</role-name>
		<principal-name>admin</principal-name>
		<group-name>administrators</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_GUEST</role-name>
		<group-name>guests</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_EMPLOYEE</role-name>
		<group-name>employees</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ACCOUNTANT</role-name>
		<group-name>accountants</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_EXTERNAL_USER</role-name>
		<group-name>external_users</group-name>
	</security-role-mapping>
	<enterprise-beans>
	</enterprise-beans>
</sun-ejb-jar>